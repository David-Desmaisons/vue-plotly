{
  "name": "vue-plotly",
  "version": "2.0.0",
  "private": false,
  "scripts": {
    "dev": "vite --open -c vite.config.js example",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "build:js": "vue-cli-service build --name vue-plotly --entry ./src/index.ts --target lib",
    "build:d.ts": "vue-tsc --pretty --declaration --emitDeclarationOnly src/index.ts",
    "test:unit": "vue-cli-service test:unit --coverage",
    "lint": "vue-cli-service lint --fix",
    "build:doc": "vue-cli-service build --entry ./example/main.js --dest docs --mode development",
    "prepublishOnly": "npm run lint && npm run build && npm run build:doc",
    "test:coverage": "vue-cli-service test:unit --coverage --verbose && codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/David-Desmaisons/vue-plotly.git"
  },
  "main": "dist/vue-plotly.umd.js",
  "module": "dist/vue-plotly.common.min.js",
  "files": [
    "dist/*.css",
    "dist/*.map",
    "dist/*.js",
    "src/*"
  ],
  "dependencies": {
    "plotly.js-dist-min": "^2.20.0"
  },
  "devDependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "@types/plotly.js": "^2.12.18",
    "@typescript-eslint/eslint-plugin": "5.57.0",
    "@typescript-eslint/parser": "5.57.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vuedoc/md": "^4.0.0-beta8",
    "autoprefixer": "^10.4.14",
    "bootstrap": "^5.2.3",
    "buffer": "^6.0.3",
    "eslint-plugin-vue": "^9.10.0",
    "font-awesome": "^4.7.0",
    "highlight.js": "^11.7.0",
    "less": "^4.1.3",
    "stream": "^0.0.2",
    "vite": "^4.2.1",
    "vue": "^3.2.47",
    "vue-server-renderer": "^2.7.14",
    "vue-tsc": "1.2.0"
  },
  "keywords": [
    "vue",
    "graphing",
    "plotting",
    "data",
    "visualization",
    "plotly"
  ],
  "license": "MIT"
}
